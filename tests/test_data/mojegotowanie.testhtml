	$(function() {

		// enable edit links
		var username = getCookie('USERNAME');
		if (username) {
            $.each( $('[data-author="'+username+'"] .recipe-edit'), function( key, value ) {
                $(this).html('<a href="' + $(this).attr('data-href') + '" title="'+ $(this).attr('data-title') +'"><i class="fa fa-edit fa-2x"></i></a>')
                $(this).show();
            });
		}

		var get_user_data_url = "/boards/get-data";
		var add_to_board_url = "/boards/board/__board_id__/add/__recipe_id__";
		var remove_from_board_url = "/boards/board/__board_id__/remove/__recipe_id__";

		function getCookie(name) {
			var value = "; " + document.cookie;
			var parts = value.split("; " + name + "=");
			if (parts.length == 2) return parts.pop().split(";").shift();
		}

		function getUserData() {
			var session = getCookie('PHPSESSID');
			if (session && sessionStorage) {
				var data = sessionStorage.getItem('user_data_' + session);
				if ((data === undefined) || (data == null) || (data == "undefined")) {
					return false;
				}
				try {
					return JSON.parse(data);
				}
				catch(e) {
					return false;
				}
			}
			return false;
		}

		function updateUserData(userData) {
			var session = getCookie('PHPSESSID');
			if (session && sessionStorage) {
				return sessionStorage.setItem('user_data_' + session, JSON.stringify(userData));
			}
		}

		function loadUserData(callback) {
			var userData = getUserData();

			if (userData) {
				callback(userData);
			} else {
				$.ajax(get_user_data_url)
					.done(function(data) {
						// console.log(data);
						if (data.redirect) {
							window.location.href = data.redirect + '?target=' + encodeURIComponent(window.location.href);
							return;
						}
						updateUserData(data['user_data']);
						callback(data['user_data']);
					})
					.fail(function (data, textStatus, jqXHR) {
						// console.log(data, textStatus, jqXHR);
					});
			}
		}

		function renderBoardsList(boardsContainer, userData, reload) {
			if (!boardsContainer.data('loaded') || reload) {
				var id = findId(boardsContainer);
				boardsContainer.empty();
                if (userData.boards) {
                    for(var i=0; i < userData.boards.length; i++) {
                        var board 	= userData.boards[i]
                            html 	= '<label><input type="checkbox" value="'+board.id+'" '+ (board.recipes && (board.recipes.indexOf(id) > -1) ? 'checked' : '' ) +' /> '+board.title+'</label>';
                        boardsContainer.append(html);
                    }
                    boardsContainer.data('loaded', true);
                    $('.btn-save').attr('disabled', false);
                }
			}
		}

		function findId(node) {
			return $(node).parents('.boards').data('id');
		}

		function addToBoard(recipe_id, board_id, callback) {
			var url = add_to_board_url
						.replace('__recipe_id__', recipe_id)
						.replace('__board_id__', board_id);
			return ajaxAction(url, callback);
		}

		function removeFromBoard(recipe_id, board_id, callback) {
			var url = remove_from_board_url
						.replace('__recipe_id__', recipe_id)
						.replace('__board_id__', board_id);
			return ajaxAction(url, callback);
		}

		function ajaxAction(url, callback) {
			$.ajax(url)
				.done(function (data) {
					if (data.redirect) {
						window.location.href = data.redirect;
					} else {
						if (data['user_data']) {
							updateUserData(data['user_data']);
						}
						if (data.message) {
							// addFlash(data.message, data.type);
						}
						if (callback) {
							callback(data);
						}
					}
				})
				.fail(function (data, textStatus, jqXHR) {
					// console.log(data, textStatus, jqXHR);
				})
			;
		}

		$(document).on('click', '.btn-save', function(event) {
            event.preventDefault();
            
            $('div.error').text('');
            
            var form = $('.panel form');
            panel = $('.boards .panel');
            boards = panel.find('.boards-container');
                
                
            if (form.find('input[type=text]').val()!='') {
                $('div.error').text('Najpierw dokończ tworzenie nowej książki kucharskiej, klikając przycisk STWÓRZ');
                return false;
            }
            
            if (!$('input[type=checkbox]:checked', boards).val()) {
                $('div.error').text('Najpierw wybierz książkę kucharską');
                return false;
            }
                
            $('div.error').text('Przepis został dodany do książki');
            
            $('.boards .panel').hide();
            return true;
		});

        //  create new board
		$(document).on('submit', '.panel form', function(event) {
			event.preventDefault();
			var form 		= $(this)
				formData 	= form.serialize();

			$.ajax(this.action, {
				method: this.method,
				data: formData,
				processData: false
			})
			.done(function(data){
				panel = $('.boards .panel');
				boards = panel.find('.boards-container');

				renderBoardsList(boards, data['user_data'], true);
				updateUserData(data['user_data']);
				form.find('input[type=text]').val(null);
                $('div.error').text('');
			})
			.fail(function (data, textStatus, jqXHR) {
				// console.log(data, textStatus, jqXHR);
			})
			;
		});

		$(document).on('change', '.panel .boards-container input', function (event) {
			var id  = findId(this);
			if (this.checked) {
				addToBoard(id, this.value);
			} else {
				removeFromBoard(id, this.value);
			}
		});


		$(document).on('click', '.add-to-cookbook', function(event){
			event.preventDefault();

			var link 		= $(this),
				panel 		= $('.boards .panel')
				userData 	= getUserData(),
				id 			= findId(this),
				boards 		= panel.find('.boards-container');

			panel.hide().show();

            if (username) {
                // load user data
                loadUserData(function(userData) {
                    panel.removeClass('loading');
                    renderBoardsList(boards, userData);
                });
                $('#add2cookbook-loggedin').find('div.error').text('');
                $('#add2cookbook-loggedin').modal();
                renderBoardsList(boards, userData);
            } else {
                $('#add2cookbook-loggedout').modal();
            }

			return false;
		});
        
	});
</script><div class="row recipe-view-classification recipe-view-classification-old"><div class="col-md-12 col-sm-12"><h2>Główne składniki:</h2><ul><li><a href="/przepisy/serek">
